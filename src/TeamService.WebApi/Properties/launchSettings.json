{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false, 
    "anonymousAuthentication": true, 
    "iisExpress": {
      "applicationUrl": "http://localhost:8444",
      "sslPort": 44325
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "api/values",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Cognito.WebApi": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "api/values",
      "applicationUrl": "https://localhost:8001;http://localhost:8000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "AWS_COGNITO_ACCESS_KEY": "1",
        "AWS_COGNITO_SECRET_ACCESS_KEY": "2",
        "AWS_COGNITO_IDENTITY_POOL_ID_KEY": "3",
        "AWS_COGNITO_USER_POOL_PROVIDER_KEY": "4",
        "AWS_COGNITO_USER_POOL_ID" : "5"
      }
    }
  }
}